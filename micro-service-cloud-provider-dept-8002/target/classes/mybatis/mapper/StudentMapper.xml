    <insert id="insertStudent">
        insert into tb_students(stu_num, stu_name, stu_gender, stu_age)
        values(#{stuNum},#{stuName},#{stuGender},#{stuAge})
    </insert>
    <delete id="deleteStudent">
        delete from tb_students where stu_num=#{stuNum}
    </delete>
</mapper>